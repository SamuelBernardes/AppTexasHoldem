    private void Abrir() {
        String[] texto = new String[12];

        try {
            texto = f.loadTxt();
        } catch (FileNotFoundException ex) {
            Logger.getLogger(Functions.class.getName()).log(Level.SEVERE, null, ex);
        } catch (IOException ex) {
            Logger.getLogger(Functions.class.getName()).log(Level.SEVERE, null, ex);
        }

        String str = "";

        for (int i = 0; i < texto.length; i++) {
            if (texto[i].equals("")) {
            } else if (i == 0) {
                str = texto[i];
            } else {
                str = str + "\n" + texto[i];
            }
        }

        txtArea.setText(str);

    }

    private void go() {

        String str = txtArea.getText();

        if (!(str == null || str.equals(""))) {
            String[] texto = new String[12];
            texto = str.split("\n");
            int numero_de_jogadores = 0;

            for (int i = 0; i < texto.length; i++) {
                if (!texto[i].equals("")) {
                    numero_de_jogadores++;
                }
            }

            players = new Player[numero_de_jogadores];

            for (int i = 0; i < players.length; i++) {
                players[i] = f.setPlayer(texto[i], players[i]);
            }

            players = f.setWinner(players);

            jFrame1.setVisible(true);
            setLabels(players);

        } else {
            JOptionPane.showMessageDialog
                    (null, "Preencha o quadro com as cartas do jogadores",
                    "AtenÃ§Ã£o", 2);
        }

    }

    private void setLabels(Player[] players) {
        String hand = "";
        String discard = "";
        String name = "";

        for (int i = 0; i < players.length; i++) {
            hand = "";
            discard = "";
            name = "";
            int k = 0;
            for (int j = 0; j < players[i].getHand().length; j++) {
                k++;
                if (k < 6) {
                    hand = hand + f.intToFace(players[i].getHand()[j].getFace())
                            + players[i].getHand()[j].getSuit() + "  ";
                } else {
                    discard = discard +
                            f.intToFace(players[i].getHand()[j].getFace())
                            + players[i].getHand()[j].getSuit() + "  ";
                }
            }

            name = f.rankString(players[i].getRank());

            if (players[i].getRank() == 1) {
                name = "'" + f.intToFace(players[i].getHand()[0].getFace())
                        + "'" + name;
            }

            switch (i) {
                case 0:
                    lhand1.setText(hand);
                    ldiscart1.setText(discard);
                    lname1.setText(name);
                    if (players[i].isWinner()) {
                        lhand1.setForeground(Color.red);
                        lname1.setForeground(Color.red);
                    }
                    break;
                case 1:
                    lhand2.setText(hand);
                    ldiscart2.setText(discard);
                    lname2.setText(name);
                    if (players[i].isWinner()) {
                        lhand2.setForeground(Color.red);
                        lname2.setForeground(Color.red);
                    }
                    break;
                case 2:
                    lhand3.setText(hand);
                    ldiscart3.setText(discard);
                    lname3.setText(name);
                    if (players[i].isWinner()) {
                        lhand3.setForeground(Color.red);
                        lname3.setForeground(Color.red);
                    }
                    break;
                case 3:
                    lhand4.setText(hand);
                    ldiscart4.setText(discard);
                    lname4.setText(name);
                    if (players[i].isWinner()) {
                        lhand4.setForeground(Color.red);
                        lname4.setForeground(Color.red);
                    }
                    break;
                case 4:
                    lhand5.setText(hand);
                    ldiscart5.setText(discard);
                    lname5.setText(name);
                    if (players[i].isWinner()) {
                        lhand5.setForeground(Color.red);
                        lname5.setForeground(Color.red);
                    }
                    break;
                case 5:
                    lhand6.setText(hand);
                    ldiscart6.setText(discard);
                    lname6.setText(name);
                    if (players[i].isWinner()) {
                        lhand6.setForeground(Color.red);
                        lname6.setForeground(Color.red);
                    }
                    break;
                case 6:
                    lhand7.setText(hand);
                    ldiscart7.setText(discard);
                    lname7.setText(name);
                    if (players[i].isWinner()) {
                        lhand7.setForeground(Color.red);
                        lname7.setForeground(Color.red);
                    }
                    break;
                case 7:
                    lhand8.setText(hand);
                    ldiscart8.setText(discard);
                    lname8.setText(name);
                    if (players[i].isWinner()) {
                        lhand8.setForeground(Color.red);
                        lname8.setForeground(Color.red);
                    }
                    break;
                case 8:
                    lhand9.setText(hand);
                    ldiscart9.setText(discard);
                    lname9.setText(name);
                    if (players[i].isWinner()) {
                        lhand9.setForeground(Color.red);
                        lname9.setForeground(Color.red);
                    }
                    break;
                case 9:
                    lhand10.setText(hand);
                    ldiscart10.setText(discard);
                    lname10.setText(name);
                    if (players[i].isWinner()) {
                        lhand10.setForeground(Color.red);
                        lname10.setForeground(Color.red);
                    }
                    break;
                case 10:
                    lhand11.setText(hand);
                    ldiscart11.setText(discard);
                    lname11.setText(name);
                    if (players[i].isWinner()) {
                        lhand11.setForeground(Color.red);
                        lname11.setForeground(Color.red);
                    }
                    break;
                default:
                    lhand12.setText(hand);
                    ldiscart12.setText(discard);
                    lname12.setText(name);
                    if (players[i].isWinner()) {
                        lhand12.setForeground(Color.red);
                        lname12.setForeground(Color.red);
                    }
            }
        }
    }

